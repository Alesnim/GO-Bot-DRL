{
  "db_file_paths": {
    "database": "data/movie_db.pkl",
    "dict": "data/movie_dict.pkl",
    "user_goals": "data/movie_user_goals.pkl"
  },
  "run": {
    "warmup_mem": 1000,
    "num_ep_run": 25000,
    "train_freq": 100,
    "max_round_num": 20,
    "success_rate_threshold": 0.3
  },
  "agent": {
    "save_weights_file_path": "weights/model.h5",
    "load_weights_file_path": "weights/model.h5",
    "vanilla": true,
    "learning_rate": 1e-3,
    "batch_size": 16,
    "dqn_hidden_size": 80,
    "epsilon_init": 0.0,
    "gamma": 0.9,
    "max_mem_size": 500000,
    "decay_rate": 0.999,
    "smooth_eps": 1e-8,
    "grad_clip_constant": 1e-3,
    "l1_reg_constant": 1e-3
  },
  "emc": {
    "slot_error_mode": 0,
    "slot_error_prob": 0.05,
    "intent_error_prob": 0.0
  }
}